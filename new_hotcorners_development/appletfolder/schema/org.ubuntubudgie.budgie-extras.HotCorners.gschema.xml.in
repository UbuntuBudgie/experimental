<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema path="/org/ubuntubudgie/budgie-extras/HotCorners/"
            id="org.ubuntubudgie.budgie-extras.HotCorners">
        <key type="i" name="pressure">
            <summary>Hotcorners pressure</summary>
            <description>Hotcorners pressure</description>
            <default>20</default>
	    <range min="0" max="100"/>
        </key>
        
        <key type="i" name="delay">
            <summary>Hotcorners delay</summary>
            <description>Hotcorners delay</description>
            <default>20</default>
	    <range min="0" max="100"/>
        </key>
        <key type="b" name="active">
            <summary>Switch hotcorners on/off</summary>
            <description>Switch hotcorners on/off</description>
            <default>false</default>
        </key>
        <key type="as" name="commands">
            <summary>Command list</summary>
            <description>The list of set commands; left, t-left, top, t-right, right, b-right, bottom, b-left, </description>
            <default>['', '', '', '','', '', '', '']</default>
        </key>
    </schema>
</schemalist>


